{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Parser",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/app/requirements_parser.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",}
        },
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"}
        }
    ]
}